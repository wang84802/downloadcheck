            {
                "name": "get-tree-YMK_AnimatedContent",
                "event": [
                    {
                        "listen": "test",
                        "script": {
                            "id": "963f30dc-604a-48d7-b47a-d7dda718dbe6",
                            "exec": [
                                "pm.test(\"Status code is 200\", function () {\r",
                                "    pm.response.to.have.status(200);\r",
                                "});\r",
                                "var jsonBody = String(responseBody);\r",
                                "var re = /\"guid\":\"[^\"]*\"/gi;\r",
                                "var match_array = jsonBody.match(re);\r",
                                "var guid_list = [];\r",
                                "var collection_guid_list = pm.collectionVariables.get(\"collection_guid_list\");\r",
                                "if(collection_guid_list && collection_guid_list.length > 0)\r",
                                "    guid_list = guid_list.concat(collection_guid_list);\r",
                                "// console.log(match_array)\r",
                                "// console.log(guid_list)\r",
                                "match_array.forEach(element =>{\r",
                                "    element = element.split(':')[1];\r",
                                "    element = element.substring(1, element.length - 1);\r",
                                "    guid_list.push(element);\r",
                                "    // console.log(element);\r",
                                "});\r",
                                "pm.collectionVariables.set(\"collection_guid_list\", guid_list);\r",
                                ""
                            ],
                            "type": "text/javascript"
                        }
                    }
                ],
